# .funlabrc is an `funlab' 0.0.0 configuration file,
# this file ought to be in /etc/ and/or your home dirctory.

log-path ${HOME}/src/funlab/x_funlab.log
icon-path ${HOME}/src/funlab/res/funlab.xpm
resource-path ${HOME}/src/funlab/res/funlab.ui
resolution-x 800
resolution-y 600
aspect 1
density 32
# stereo false
3D true
show-axis false
multi-formula false
domain-lower-bound -10
domain-upper-bound 10
range-lower-bound -10
range-upper-bound 10

# End of configuration file.

